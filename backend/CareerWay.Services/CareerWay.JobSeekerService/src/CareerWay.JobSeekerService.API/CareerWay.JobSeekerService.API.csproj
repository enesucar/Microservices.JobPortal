		<PackageReference Include="Serilog.Sinks.Seq" Version="9.0.0" />
		<PackageReference Include="Steeltoe.Discovery.Eureka" Version="3.2.8" />

	</ItemGroup> 
	
  <ItemGroup>
    <ProjectReference Include="..\CareerWay.JobSeekerService.Infrastructure\CareerWay.JobSeekerService.Infrastructure.csproj" />
